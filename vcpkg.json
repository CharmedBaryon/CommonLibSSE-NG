{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "commonlibsse-ng",
  "version-semver": "3.5.0",
  "port-version": 0,
  "description": "A fork of CommonLibSSE with advanced features for modern SKSE development.",
  "homepage": "https://github.com/CharmedBaryon/CommonLibSSE",
  "license": "MIT",
  "supports": "windows & x64",
  "dependencies": [
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "fast-cpp-csv-parser",
    "fmt",
    "rapidcsv",
    "rsm-binary-io",
    "spdlog"
  ],
  "features": {
    "tests": {
      "description": "Build tests for CommonLibSSE.",
      "dependencies": [
        "catch2"
      ]
    }
  },
  "default-features": [
    "tests"
  ],
  "overrides": [
    {
      "name": "catch2",
      "version": "2.13.9#1"
    },
    {
      "name": "fmt",
      "version": "8.1.1#1"
    },
    {
      "name": "rapidcsv",
      "version": "8.53"
    },
    {
      "name": "spdlog",
      "version": "1.10.0#1"
    }
  ],
  "builtin-baseline": "faed44dfa013088fe1910908a8a65887622f412f"
}
